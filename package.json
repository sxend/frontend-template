{
  "name": "frontend-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf target",
    "test:unit": "ava --verbose ./target/tsc/test/unit/**/*.js",
    "test:e2e": "ava --verbose ./target/tsc/test/e2e/**/*.js",
    "test": "npm run build && npm run test:unit && npm run test:e2e",
    "build": "tsc && webpack --config ./webpack.config.js",
    "build:watch": "tsc --watch & webpack --watch --config ./webpack.config.js"
  },
  "author": "sxend",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.22.0",
    "awesome-typescript-loader": "^3.2.3",
    "nightmare": "^2.10.0",
    "preprocess-loader": "^0.2.2",
    "typescript": "^2.5.3",
    "webpack": "^3.7.1"
  },
  "dependencies": {
    "es6-promise": "^4.1.1"
  }
}
